algoritmo "Calcular a  area  do retângulo"
// Função :
// Autor :
// Data : 09/03/2023
// Seção de Declarações 
var
area , base , altura :inteiro
inicio
// Seção de Comandos 
escreval("digite uma base:")
leia(base)

escreval("digite uma altura:")
leia(altura)

area <- (base*altura)

escreval("a area do retângulo e :" , area )

fimalgoritmo
